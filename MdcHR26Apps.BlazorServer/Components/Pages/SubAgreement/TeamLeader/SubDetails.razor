@page "/SubAgreement/TeamLeader/SubDetails/{Id:long}"

<PageTitle>세부 업무 리스트</PageTitle>

<h3>세부 업무 리스트</h3>
<hr />

@if (processDb == null)
{
    <p><em>Loading...</em></p>
}
else
{
    <button class="btn btn-secondary" @onclick="(() => MoveTeamLeaderSubAgreementDetailsPage(processDb.Pid))">목록</button>
    <hr />
}

@if (model == null)
{
    <p><em>Loading...</em></p>
}
else if (model.Count == 0)
{
    <p><em>No Data</em></p>
}
else
{
    <TaskListTable tasklist="model"></TaskListTable>
}